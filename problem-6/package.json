{
  "name": "backend_template",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main",
    "test:memory": "DISABLE_SCHEDULE=true dotenv -e .env.dev -- node --max-old-space-size=70 dist/main.js",
    "start:dev": "dotenv -e .env.dev -- nest start --watch",
    "start:test": "dotenv -e .env.test -- node dist/main",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "deploy": "yarn run build && serverless deploy",
    "undeploy": "serverless remove"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.729.0",
    "@aws-sdk/client-sqs": "^3.864.0",
    "@blazity/nest-file-fastify": "^1.0.0",
    "@fastify/multipart": "^9.0.2",
    "@fastify/static": "^8.0.4",
    "@nestjs/cache-manager": "^2.3.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/event-emitter": "^2.1.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^11.0.1",
    "@nestjs/mongoose": "^10.1.0",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-fastify": "^11.0.1",
    "@nestjs/platform-socket.io": "^11.0.7",
    "@nestjs/schedule": "^4.1.2",
    "@nestjs/swagger": "^11.0.0",
    "@nestjs/websockets": "^11.0.7",
    "aws-serverless-fastify": "^3.1.0",
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "cache-manager-redis-yet": "^5.1.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.7",
    "firebase-admin": "^13.0.2",
    "ioredis": "^5.4.2",
    "lambda-multipart-parser": "^1.0.1",
    "minio": "^8.0.3",
    "moment": "^2.30.1",
    "mongo-sanitize": "^1.1.0",
    "mongodb": "^6.20.0",
    "mongoose": "^8.9.5",
    "nodemailer": "^6.9.16",
    "pusher": "^5.2.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^11.0.1",
    "@types/aws-lambda": "^8.10.150",
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "serverless": "^4.18.0",
    "serverless-api-gateway-throttling": "^2.0.3",
    "serverless-prune-plugin": "^2.1.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
